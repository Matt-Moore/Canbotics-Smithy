		<% detailsProducts.forEach(function(category) {%><section>
			<h2><a href="<%= category.dataPage[detailsPage.langCode].path %>"><%= category.dataPage[detailsPage.langCode].title %></a></h2>
			<% category.subcategory.forEach(function(subcategory) { %>
			<h3><% if (subcategory.products.length == 1) { %><%= subcategory[detailsPage.langCode][0] %><% } else { %><%= subcategory[detailsPage.langCode][1] %><% }; %></h3>
			<ul class="listWeapon">
				<% subcategory.products.forEach(function(product) { %><li>
					<a href="<%= category.dataPage[detailsPage.langCode].path + "/" + product[detailsPage.langCode].prodUri %>">
						<h3><%= product.attr.name %></h3>
						<img src="<%= dataSite.uriPath.rootAsset %>img/<%= detailsPage.navSegment %>/<%= product.en.prodUri%>-icon-90.png" width="90" height="90" alt="">
					</a>
					<p><%= product[detailsPage.langCode].prodBlurb.split(".")[0] %></p>
				</li><% }); %>
			</ul><% }); %>	
		</section>
		<% }); %>